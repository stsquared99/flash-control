{
  "manifest_version": 2,

  "name": "Flash Control",
  "description": "This extension allows you to easily control your flash player exceptions",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "contentSettings",
    "https://ajax.googleapis.com/"
  ]
}